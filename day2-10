Vowels or Consonants
#include <stdio.h>

int main() {
    char t;
    scanf(" %c", &t); 

    if (t == 'a' || t == 'e' || t == 'i' || t == 'o' || t == 'u' ||
        t == 'A' || t == 'E' || t == 'I' || t == 'O' || t == 'U') {
        printf("The Character %c is Vowel", t);
    } else {
        printf("The Character %c is Consonant", t);
    }

    return 0;
}

