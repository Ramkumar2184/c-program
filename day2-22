Sum of Digits
#include <stdio.h>

int main() {
    int number;


    scanf("%d", &number);

    
    if (number >= 10 && number < 100) {
        int digit1 = number / 10;
        int digit2 = number % 10;
        int sum = digit1 + digit2;

        
        printf("Sum of Digit %d is %03d\n", number, sum);
    } else {
        printf("Invalid Input\n");
    }

    return 0;
}
