Float Formation
#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
   float a;
    scanf("%f",&a);
    printf("%0.6f\n",a);
    printf("%0.4f\n",a);
    printf("%0.2f\n",a);
    printf("%d\n",(int)ceil(a));
    
    return 0;
}
