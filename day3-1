Same or Not
#include <stdio.h>

int main() {
    int n, num;
    int x=0;

    
    scanf("%d", &n);


    for(int i = 0; i < n; i++) {
        scanf("%d", &num);
        
        if(num % 2 == 0)
           x++ ;
    }
if(x==n)
    printf("Even");
    else
        printf("Mixed");
    
   

    return 0;
}

