Factorial Number
#include <stdio.h>


long long factorial(int n) {
    long long result = 1;
    for (int i = 1; i <= n; i++) {
        result *= i;
    }
    return result;
}

int main() {
    int n;
    
    
    scanf("%d", &n);
    
    
    if (n < 0) {
        printf("Invalid Input\n");
    } else {
    
        printf("The Factorial of %d is %lld\n", n, factorial(n));
    }
    
    return 0;
}

